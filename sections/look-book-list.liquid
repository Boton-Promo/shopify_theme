{% render 'section-style' %}

<style>
    #shopify-section-{{ section.id }} .ap-productlist__inner--scroller {
        gap: 0;
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__dot {
        background: {{ section.settings.dot_inner_background }};
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__dot:hover {
        background-color: var(--primary);
    }
    [dir=ltr] #shopify-section-{{ section.id }} .ap-lookbookshop__product--reverse {
        right: auto;
        left: calc(100% + 25px);
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__product:before {
        display: none;
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__product {
        min-width: 332px;
        width: 100%;
    }
    [dir=ltr] #shopify-section-{{ section.id }} .ap-lookbookshop__product-image {
        margin-right: 14px;
    }
    [dir=ltr] #shopify-section-{{ section.id }} .ap-lookbookshop__product {
        padding: 20px;
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__product-image {
        flex: none;
        width: 95px;
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__product-title {
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        overflow: hidden;
        margin-bottom: 10px;
        font-weight: 400;
        line-height: 22px;
        font-size: 18px;
        font-family: var(--hd-font-family);
        color: var(--color-accent-5);
    }
    #shopify-section-{{ section.id }} .ap-lookbookshop__product-wrapper {
        z-index: unset;
    }
    #shopify-section-{{ section.id }} .banner-carousel-content-text {
        margin: -110px 0 0;
        text-align: center;
        background-color: {{ section.settings.bg_color_item }};
        position: relative;
        max-width: 600px;
        margin-left: auto;
        margin-right: auto;
        padding: 40px 0 40px;
    }
    #shopify-section-{{ section.id }} .description {
        font-size: 16px;
        line-height: 28px;
        font-family: var(--body-font-family);
        margin: 0;
        text-transform: none;
        font-weight: 400;
    }
    #shopify-section-{{ section.id }} .heading-center.h2 {
        margin: 0 0 10px;
    }
    #shopify-section-{{ section.id }} .ap-text-loaderbutton {
        justify-content: center;
    }
    #shopify-section-{{ section.id }} .ap-text-loaderbutton svg {
        display: none;
    }

    @media (max-width: 1500px) {
      [dir=ltr] #shopify-section-template--21686505603383__e18d94d5-1d8c-43de-b7c6-51f8685639b4 .ap-lookbookshop__product--reverse {
        left: 50%;
        transform: translateX(-50%);
        top: 110%;
      }
    }
    @media (max-width: 1299px) {
      #shopify-section-{{ section.id }} .banner-carousel-content-text {
        max-width: 450px;
      }
    }
    @media (max-width: 991px) {
      #shopify-section-{{ section.id }} .banner-carousel-content-text {
        margin: 0;
        max-width: 100%;
        padding: 15px 0 0;
      }
    }
    @media (max-width: 767px) {
      #shopify-section-{{ section.id }} .shopify-payment-button__button--unbranded {
        width: 100%;
      }
    }

    {% for block in section.blocks %}
        @media screen and (max-width: 575px) {
            #shopify-section-{{ section.id }} .ap-productlist__inner--scroller {
                --section-products-per-row: {{ section.settings.products_per_row_mobile }};
                grid-auto-columns: calc((100% - var(--ap-productlist-column-gap) * (1 - 1)) / 1);
            }
        }

        @media (min-width:576px) and (max-width: 767px) {
            #shopify-section-{{ section.id }} {
                --section-products-per-row: {{ section.settings.products_per_row_ipad }};
            }
            #shopify-section-{{ section.id }} .ap-productlist__inner--scroller{
                margin:0;
                padding:0px;
                grid-auto-columns:calc((100% - var(--ap-productlist-column-gap) * ({{ section.settings.products_per_row_ipad }} - 1)) / {{ section.settings.products_per_row_ipad }});
            }
        }
    
        @media (min-width: 768px) and (max-width: 1199px) {
            #shopify-section-{{ section.id }} {
                --section-products-per-row: {{ section.settings.products_per_row_tablet }}
            }
            #shopify-section-{{ section.id }} .ap-productlist__inner--scroller{
                margin:0;
                padding:0;
                grid-auto-columns:calc((100% - var(--ap-productlist-column-gap) * ({{ section.settings.products_per_row_tablet }} - 1)) / {{ section.settings.products_per_row_tablet }});
            }
        }
        
        @media screen and (min-width: 1200px) {
            #shopify-section-{{ section.id }} {
                --section-products-per-row: {{ section.settings.products_per_row_desktop }};
            }
            #shopify-section-{{ section.id }} .ap-productlist__inner--scroller{
                margin:0;
                padding:0;
                grid-auto-columns:calc((100% - var(--ap-productlist-column-gap) * ({{ section.settings.products_per_row_desktop }} - 1)) / {{ section.settings.products_per_row_desktop }});
            }
        }

        {% if block.settings.product_1 %}
            #block-{{ block.id }}-1 {
                top: {{ block.settings.product_1_top }}%;
                left: {{ block.settings.product_1_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-1 {
                    top: {{ block.settings.product_1_top_tablet }}%;
                    left: {{ block.settings.product_1_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_2 %}
            #block-{{ block.id }}-2 {
                top: {{ block.settings.product_2_top }}%;
                left: {{ block.settings.product_2_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-2 {
                    top: {{ block.settings.product_2_top_tablet }}%;
                    left: {{ block.settings.product_2_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_3 %}
            #block-{{ block.id }}-3 {
                top: {{ block.settings.product_3_top }}%;
                left: {{ block.settings.product_3_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-3 {
                    top: {{ block.settings.product_3_top_tablet }}%;
                    left: {{ block.settings.product_3_left_tablet }}%;
                }
            }
        {% endif %}
    {% endfor %}
</style>

