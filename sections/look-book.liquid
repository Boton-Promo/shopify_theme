{{ 'look-book.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}
   
{% style %}
    #shopify-section-{{ section.id }} {
      --color-dot: {{ section.settings.color_dot }};
      --bg-color: {{ section.settings.bg_color_product }};
      --bg-gradient-bottom: {{ section.settings.bg_gradient_section }};
      position: relative;
      z-index: 99;
    }
    
    #shopify-section-{{ section.id }} .ap-text-loaderbutton {
      flex-direction: row;
    }

    #shopify-section-{{ section.id }} .block-product-list {
      min-width: 33.333%;
      background-image: url({{ section.settings.bg_product | image_url: width: 2000 }});
      background-size: cover;
      background-position: center;
      display: grid;
      align-items: center;
    }
    
    {% for block in section.blocks %}
        #block-{{ block.id }} {
            --section-dot-inner-background: {{ section.settings.dot_inner_background | color_extract: 'red' }}, {{ section.settings.dot_inner_background | color_extract: 'green' }}, {{ section.settings.dot_inner_background | color_extract: 'blue' }};
            --section-dot-background: {{ section.settings.dot_background | color_extract: 'red' }}, {{ section.settings.dot_background | color_extract: 'green' }}, {{ section.settings.dot_background | color_extract: 'blue' }};
        }
        {% if block.settings.product_1 %}
            #block-{{ block.id }}-1 {
                top: {{ block.settings.product_1_top }}%;
                left: {{ block.settings.product_1_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-1 {
                    top: {{ block.settings.product_1_top_tablet }}%;
                    left: {{ block.settings.product_1_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_2 %}
            #block-{{ block.id }}-2 {
                top: {{ block.settings.product_2_top }}%;
                left: {{ block.settings.product_2_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-2 {
                    top: {{ block.settings.product_2_top_tablet }}%;
                    left: {{ block.settings.product_2_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_3 %}
            #block-{{ block.id }}-3 {
                top: {{ block.settings.product_3_top }}%;
                left: {{ block.settings.product_3_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-3 {
                    top: {{ block.settings.product_3_top_tablet }}%;
                    left: {{ block.settings.product_3_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_4 %}
            #block-{{ block.id }}-4 {
                top: {{ block.settings.product_4_top }}%;
                left: {{ block.settings.product_4_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-4 {
                    top: {{ block.settings.product_4_top_tablet }}%;
                    left: {{ block.settings.product_4_left_tablet }}%;
                }
            }
        {% endif %}
        {% if block.settings.product_5 %}
            #block-{{ block.id }}-5 {
                top: {{ block.settings.product_5_top }}%;
                left: {{ block.settings.product_5_left }}%;
            }
            @media screen and (max-width: 740px) {
                #block-{{ block.id }}-5 {
                    top: {{ block.settings.product_5_top_tablet }}%;
                    left: {{ block.settings.product_5_left_tablet }}%;
                }
            }
        {% endif %}
    {% endfor %}
{% endstyle %}

<section class="look-book">
  <div class="{{ section.settings.setwidthout }}">
    <header class="section__header ">
      <div class="text-container">
        <h3 class="heading h{{ section.settings.tab_heading_size }} mb-60">{{ section.settings.tab_heading }}
        </h3>
        {% if section.settings.tab_description != blank %}
        <div class="heading-description">
          {{ section.settings.tab_description }}
        </div>
        {% endif %}
      </div>
    </header>
    <ap-lookbookshop reveal-on-scroll="" class="ap-lookbookshop ap-object-loaded" style="opacity: 1;">
      <div class="ap-lookbookshop__item-list">
          {% for block in section.blocks %}
              <ap-lookbookshop-item id="block-{{ block.id }}" class="ap-lookbookshop__item" {% if forloop.index != 1 %} hidden="" {% endif %}>
                {% if block.settings.show_product_list %}
                  <div class="block-product-list">
                    {% if section.settings.title != blank %}
                    <h3 style="color: var(--primary-2);" class="heading h4 mb-30">{{ block.settings.title }}</h3>
                    {% endif %}
                    <div class="featured-collections lookbook-product">
                      <ap-product-carousel reveal-on-scroll="" auto-play="" transition-type="sweep"
                        class="product-carousel-content ap-product-carousel">
                        <div class="ap-product-carousel__wrapper">
                          {%- if block.settings.collection != blank -%}
                          {% for product in block.settings.collection.products limit: block.settings.limit %}
                          <product-carousel class="product-carousel" {% if forloop.index !=1 %}hidden="" {% endif %}>
                            <div class="product-carousel-box" reveal-visibility="">
                              {% render 'product-grid', product: product, show_rating: block.settings.show_rating %}
                            </div>
                          </product-carousel>
                          {% endfor %}
                          {%- else -%}
                          <div class="product-items">
                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                            {{ 'onboarding.product_title' | t }}
                          </div>
                          {%- endif -%}
                  
                        </div>
                        {% if block.settings.show_lookbook__nav %}
                        <div class="ap-product-carousel__nav">
                          <ap-pagedots class="lookbook__nav container">
                            {% for product in block.settings.collection.products limit: block.settings.limit %}
                            <button class="lookbook__progress-bar dots-nav__item tap-area"
                              ap-currentaria="{% if forloop.index == 1 %}true{% else %}false{% endif %}">
                              <span class="visually-hidden">{{ forloop.index }}</span>
                            </button>
                            {% endfor %}
                          </ap-pagedots>
                        </div>
                        {% endif %}
  
                        <ap-button-nextprev class="ap-productlist__ap-nextprev hd-lap hd-pocket" style="--smallest-image-aspect-ratio: 1.0">
                          <button class="ap-productlist__arrow ap-nextap-buttonprev ap-nextap-buttonprev--prev">
                            <span class="visually-hidden">Previous</span>
                            <svg class="icon icon--nav-arrow-left  icon--direction-aware " width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd" clip-rule="evenodd" d="M12.1086 0.23385C11.7847 -0.07795 11.2603 -0.07795 10.9372 0.23385L4.08682 6.8444C3.43981 7.46879 3.43981 8.48173 4.08682 9.10612L10.9869 15.7659C11.3075 16.0745 11.8253 16.0787 12.15 15.7741C12.4822 15.4631 12.4855 14.9504 12.1591 14.6346L5.84395 8.54088C5.52003 8.22829 5.52003 7.72222 5.84395 7.40963L12.1086 1.36432C12.4325 1.05252 12.4325 0.546442 12.1086 0.23385Z" fill="#222222"/>
                            </svg>

                          </button>
                          <button class="ap-productlist__arrow ap-nextap-buttonprev ap-nextap-buttonprev--next">
                            <span class="visually-hidden">Next</span>
                            <svg  class="icon icon--nav-arrow-right  icon--direction-aware " width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd" clip-rule="evenodd" d="M3.8914 0.23385C4.21532 -0.07795 4.73972 -0.07795 5.06281 0.23385L11.9132 6.8444C12.5602 7.46879 12.5602 8.48173 11.9132 9.10612L5.01311 15.7659C4.6925 16.0745 4.17474 16.0787 3.85 15.7741C3.51779 15.4631 3.51449 14.9504 3.84089 14.6346L10.1561 8.54088C10.48 8.22829 10.48 7.72222 10.1561 7.40963L3.8914 1.36432C3.56748 1.05252 3.56748 0.546442 3.8914 0.23385Z" fill="#222222"/>
                            </svg>

                          </button>
                        </ap-button-nextprev>
                  
                      </ap-product-carousel>
                    </div>
                  </div>
                  {% endif %}
                <div class="block-lookbook-content">         
                  <div class="ap-lookbookshop__image-wrapper">
                    {% if block.settings.image_wrapper != blank %}
                      <img loading="lazy" sizes="100vw" reveal="" class="ap-lookbookshop__image hd-pocket" alt="" 
                          src="{{ block.settings.image_wrapper | image_url: width: block.settings.image_wrapper.width }}" 
                          srcset="
                              {%- if block.settings.image_wrapper.width >= 500 -%}{{ block.settings.image_wrapper | image_url: width: 500 }} 500w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 700 -%}{{ block.settings.image_wrapper | image_url: width: 700 }} 700w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 900 -%}{{ block.settings.image_wrapper | image_url: width: 900 }} 900w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 1100 -%}{{ block.settings.image_wrapper | image_url: width: 1100 }} 1100w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 1500 -%}{{ block.settings.image_wrapper | image_url: width: 1500 }} 1500w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 2000 -%}{{ block.settings.image_wrapper | image_url: width: 2000 }} 2000w,{%- endif -%}
                              {%- if block.settings.image_wrapper.width >= 2500 -%}{{ block.settings.image_wrapper | image_url: width: 2500 }} 2500w,{%- endif -%}
                              {{ block.settings.image_wrapper | image_url }} {{ block.settings.image_wrapper.width }}w
                          "
                          style="opacity: 1;" 
                          width="{{ block.settings.image_wrapper.width }}" height="{{ block.settings.image_wrapper.width }}">
                    {% else %}
                      {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                    {% endif %}
                      <img loading="lazy" sizes="100vw" reveal="" class="ap-lookbookshop__image hd-lap-and-up" alt="" 
                          src="{{ block.settings.image_wrapper_tablet | image_url: width: block.settings.image_wrapper_tablet.width }}" 
                          srcset="
