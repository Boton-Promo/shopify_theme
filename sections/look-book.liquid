                                <line x1="9.5" y1="2.18557e-08" x2="9.5" y2="5" stroke="#DE8A26" />
                                <line x1="9.5" y1="14" x2="9.5" y2="19" stroke="#DE8A26" />
                                <line x1="4.37114e-08" y1="9.5" x2="5" y2="9.5" stroke="#DE8A26" />
                                <line x1="14" y1="9.5" x2="19" y2="9.5" stroke="#DE8A26" />
                                <circle cx="9.5" cy="9.5" r="7" stroke="#DE8A26" />
                                <circle cx="9.5" cy="9.5" r="2.5" fill="#DE8A26" />
                              </svg>
                              <span class="visually-hidden">{{ block.settings.product_1.title }}</span>
                          </button>
      
                          <button type="button" is="toggle-button" reveal="" class="ap-lookbookshop__dot tap-area hd-tablet-and-up" ap-controlsaria="block-{{ block.id }}-1-popover" ap-expanded-aria="{% if block.settings.product_1.product_1_show %}true{% else %}false{% endif %}" style="opacity: 1;">
                              <svg xmlns="http://www.w3.org/2000/svg" width="19" height="19" viewBox="0 0 19 19" fill="none">
                                <line x1="9.5" y1="2.18557e-08" x2="9.5" y2="5" stroke="#DE8A26" />
                                <line x1="9.5" y1="14" x2="9.5" y2="19" stroke="#DE8A26" />
                                <line x1="4.37114e-08" y1="9.5" x2="5" y2="9.5" stroke="#DE8A26" />
                                <line x1="14" y1="9.5" x2="19" y2="9.5" stroke="#DE8A26" />
                                <circle cx="9.5" cy="9.5" r="7" stroke="#DE8A26" />
                                <circle cx="9.5" cy="9.5" r="2.5" fill="#DE8A26" />
                              </svg>
                              <span class="visually-hidden">{{ block.settings.product_1.title }}</span>
                          </button>
      
                          <ap-elementopen id="block-{{ block.id }}-1-product" class="ap-lookbookshop__product ap-lookbookshop__product--reverse hd-phone" {% if block.settings.product_1.product_1_show %}open=""{% endif %}>
                              <img class="ap-lookbookshop__product-image" loading="lazy" sizes="72px" alt="{{ block.settings.product_1.title }}" 
                                  src="{{ block.settings.product_1.featured_media | image_url: width: product.featured_media.width }}" 
                                  srcset="
                                      {%- if block.settings.product_1.featured_media >= 72 -%}{{ block.settings.product_1.featured_media | image_url: width: 72 }} 72w,{%- endif -%}
                                      {%- if block.settings.product_1.featured_media >= 144 -%}{{ block.settings.product_1.featured_media | image_url: width: 144 }} 144w,{%- endif -%}
                                      {%- if block.settings.product_1.featured_media >= 216 -%}{{ block.settings.product_1.featured_media | image_url: width: 216 }} 216w,{%- endif -%}
                                      {%- if block.settings.product_1.featured_media >= 288 -%}{{ block.settings.product_1.featured_media | image_url: width: 288 }} 288w,{%- endif -%}
                                  "
                                  width="{{ block.settings.product_1.featured_media.width }}" height="{{ block.settings.product_1.featured_media.width }}">
                              <div class="ap-lookbookshop__product-info">
                                  <a href="/products/{{ block.settings.product_1.handle }}" class="ap-lookbookshop__product-title">{{ block.settings.product_1.title }}</a>
      
                                  <div class="ap-lookbookshop__product-bottom-wrapper">
                                      <span class="price">
                                          <span class="visually-hidden">Sale price</span>{{ block.settings.product_1.price | money }}
                                      </span>
                                      <button is="toggle-button" class="ap-lookbookshop__product-link link text--subdued" ap-controlsaria="block-{{ block.id }}-1-drawer" ap-expanded-aria="{% if block.settings.product_1.product_1_show %}true{% else %}false{% endif %}">Quick view</button>
                                  </div>
                              </div>
                          </ap-elementopen>

                          <ap-popoverquickbuy href="/products/{{ block.settings.product_1.handle }}?view=quick-buy-popover" id="block-{{ block.id }}-1-popover" class="popover popover--quick-buy"></ap-popoverquickbuy>
